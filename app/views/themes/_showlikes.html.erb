<%= comment.commentupvotes.count %>
<% if comment.get_commentupvote_of_user(current_user) %>
<%= link_to commentupvote_path(comment.get_commentupvote_of_user(current_user)), method: :delete, remote: true do  %>
<i  id="heart-like" class="fas fa-heart"></i>
<% end %>
<% else %>
<%= link_to comment_commentupvotes_path(comment), method: :post, remote: true  do %>
    <i id="heart-like" class="far fa-heart"></i>
<% end %>
<% end %>